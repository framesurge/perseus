{
    "languageSettings": [
        {
            "languageId": "markdown",
            "caseSensitive": true
        },
        {
            "languageId": "rust",
            "includeRegExpList": [
                "\/\/(\/)? .*"
            ]
        }
    ],
    "dictionaryDefinitions": [
        {
            "name": "project-words",
            "path": "./.project-words.txt",
            "addWords": true
        }
    ],
    "dictionaries": [ "project-words" ],
    "ignorePaths": [ "target", "dist", "node_modules", "/.project-words.txt", "/CHANGELOG.md" ]
}
